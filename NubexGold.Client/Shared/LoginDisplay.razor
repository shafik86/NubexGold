@inject NavigationManager Navigation

<AuthorizeView>
    <Authorized>
        <MudLink Href="Identity/Account/Manage"><MudChip Icon="@Icons.Material.Filled.Person" Color="Color.Secondary" > @context.User.Identity?.Name</MudChip></MudLink>
        <MudLink Href="Identity/Account/LogOut"><MudChip Icon="@Icons.Material.Filled.Logout" Color="Color.Secondary">Log Out</MudChip></MudLink>
    </Authorized>
    <NotAuthorized>
        <MudLink Href="Identity/Account/Register" ><MudChip Icon="@Icons.Material.Filled.AppRegistration" Color="Color.Info">Register</MudChip></MudLink>
        <MudLink Href="Identity/Account/Login"><MudChip Icon="@Icons.Material.Filled.Login" Color="Color.Success" >Login</MudChip></MudLink>
     
    </NotAuthorized>
</AuthorizeView>
